<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lplb.modular.mapper.DnaMethylationMapper">

    <select id="top10GenesInDnaMethylation" resultType="com.lplb.modular.model.vo.StatisticsVo">
        SELECT
            m.gene_name AS `key`,
            COUNT(m.gene_name) AS `value`
        FROM in_dna_methylation m
        GROUP BY m.gene_name
        ORDER BY `value` DESC
        LIMIT 10
    </select>
</mapper>

